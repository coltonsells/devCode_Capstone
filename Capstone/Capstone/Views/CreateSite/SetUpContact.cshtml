@model Capstone.Models.Company
@{
    ViewData["Title"] = "SetUpContact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SetUpContact</h2>
@using (Html.BeginForm())
{
<div class="container">
    <div>
        <h3>
            We've added an email form to your contact page. Customers can now contact you Via email.
        </h3>
        @Html.HiddenFor(x => x.Id)
    </div>
    <button type="submit" class="btn btn-primary">Continue</button>
</div>
}